    <webLinks>
        <fullName>Clone_Quote_Detail</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Clone</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <url>{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteEdit?id={!Quote__c.Id}&amp;oppId={!Quote__c.OpportunityId__c}&amp;clone=true</url>
