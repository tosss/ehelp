    <webLinks>
        <fullName>Clone_Quote_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clone Quote</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

var records = {!GETRECORDIDS($ObjectType.Quote__c)};
var isOpp = &apos;{!Opportunity.Id}&apos;.length &gt; 0;

if (records.length &gt; 1) {
	alert(&apos;Please select only one quote to clone.&apos;);
} else if (records.length == 1) {
	var canEdit = sforce.apex.execute(&apos;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&apos;, &apos;canEditQuote&apos;, {quoteId:records[0]});
	if (canEdit == &apos;true&apos;) {
		if (isOpp) {
			window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteEdit?id=&quot; + records[0] + &quot;&amp;oppId={!Opportunity.Id}&amp;clone=true&quot;; 
		} else {
			window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteEdit?id=&quot; + records[0] + &quot;&amp;actId={!Account.Id}&amp;clone=true&quot;; 
		}
	} else {
		alert(&apos;You do not have permission to clone that quote.  Please contact your system administrator.&apos;);
	}
} else {
	if (isOpp) {
		window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteCloneSelection?oppId={!Opportunity.Id}&quot;;
	} else {
		window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteCloneSelection?actId={!Account.Id}&quot;;
	}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Move_Quote_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Move Quote</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Quote__c)};

var moveQuote = false;
if (records == null || records.length == 0) {
	alert(&quot;Please check the box next to the quote that you would like to move before clicking the &apos;Move&apos; button.&quot;);
} else if (records.length &gt; 1) {
	alert(&quot;Please select only one quote to move.&quot;);
} else if (sforce.apex.execute(&apos;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&apos;, &apos;canEditQuote&apos;, {quoteId:records[0]}) != &apos;true&apos;) {
	alert(&apos;You do not have permission to move that quote.  Please contact your system administrator.&apos;);
} else {
	var result = sforce.connection.query(&quot;Select {!$Setup.NamespaceSettings__c.Underscore_Prefix__c}Is_Primary__c from {!$Setup.NamespaceSettings__c.Underscore_Prefix__c}Quote__c where Id = &apos;&quot; + records[0] + &quot;&apos;&quot;);
	var quote = result.getArray(&quot;records&quot;);
	if (quote[0].Is_Primary__c == &apos;true&apos;) {
		moveQuote = confirm(&quot;This quote is marked as the primary quote for its current opportunity.  Moving this quote will remove the associated opportunity products from the opportunity and may affect the value of the opportunity.  Alternatively, you could keep this quote on the current opportunity and create a clone of it in the new opportunity.  Do you want to continue to move the quote and remove the associated opportunity products from the current opportunity?&quot;);
	} else {
		moveQuote = true;
	}
}

if (moveQuote) {
	window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteMove?id=&quot; + records[0] + &quot;&amp;from_list=true&quot;; 
}</url>
    </webLinks>
    <webLinks>
        <fullName>New_Quote_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Quote</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/apex.js&quot;)}

var urlParams = &apos;&apos;;
if (&apos;{!Opportunity.Id}&apos;.length &gt; 0) {
	urlParams += &apos;?oppId={!Opportunity.Id}&apos;;
}
if (&apos;{!Account.Id}&apos;.length &gt; 0) {
	urlParams += urlParams.length &gt; 0 ? &apos;&amp;&apos; : &apos;?&apos;;
	urlParams += &apos;actId={!Account.Id}&apos;;
}

var onlyActiveSiteId = sforce.apex.execute(&apos;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&apos;, &apos;getOnlyActiveSiteId&apos;, {});

if (onlyActiveSiteId[0] != null) {
	urlParams += &apos;&amp;siteId=&apos; + onlyActiveSiteId;
	window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteEdit&quot; + urlParams;
} else {
	window.parent.location.href = &quot;{!URLFOR($Site.Prefix)}/apex/{!$Setup.NamespaceSettings__c.Underscore_Prefix__c}QuoteCreate&quot; + urlParams;
}</url>
    </webLinks>
    <webLinks>
        <fullName>Set_Primary_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set as Primary</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Quote__c)};
var isOpp = &apos;{!Opportunity.Id}&apos;.length &gt; 0;

var setAsPrimary = false;
if (records == null || records.length == 0) {
	alert(&quot;Please check the box next to the quote that you would like to be the primary quote before clicking the &apos;Set as Primary&apos; button.&quot;);
} else if (records.length &gt; 1) {
	alert(&quot;Please select only one quote to be the primary quote.&quot;);
} else if (sforce.apex.execute(&apos;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&apos;, &apos;canEditQuote&apos;, {quoteId:records[0]}) != &apos;true&apos;) {
	alert(&apos;You do not have permission to set that quote as primary.  Please contact your system administrator.&apos;);
} else if ((sforce.apex.execute(&apos;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&apos;, &apos;opportunityOfAccountHasLineItems&apos;, {quoteId:records[0]}) == &apos;true&apos;)) {
	if (isOpp) {
		setAsPrimary = confirm(&quot;Setting a new quote as the primary quote will delete all of the opportunity products on this opportunity which are associated with the current primary quote.  Do you want to continue?&quot;);
	} else {
		setAsPrimary = confirm(&quot;Setting a new quote as the primary quote will delete all of the opportunity products on the opportunity that this quote belongs to which are associated with the current primary quote.  Do you want to continue?&quot;);
	}
} else {
	setAsPrimary = true;
}

if (setAsPrimary) {
	var result = sforce.apex.execute(&quot;{!$Setup.NamespaceSettings__c.Dot_Prefix__c}BigMachinesFunctionLibrary&quot;, &quot;setQuoteAsPrimary&quot;, {quoteId:records[0]});
	var hrefVal = window.parent.location.href;
	var hashIndex = hrefVal.indexOf(&apos;#&apos;);
	if (hashIndex &gt; -1) {
		hrefVal = hrefVal.substr(0, hashIndex);
	}
	window.parent.location.href = hrefVal;
}</url>
    </webLinks>
