        }

        .colorTest {
            height: 20px;
            width: 20px;
            border-radius: 3px;
            margin: 4px !important;
            background: rgb(0, 255, 0);
        }
        /**End March Release*/
		/* Related Lists */
		#relatedlist-addnew-wrapper {
			text-align: right;
			padding: 5px 0 !important;
		}
		#relatedlist-addnew-wrapper .ui-autocomplete-input {
			width: 350px;
			box-sizing: border-box;
		}
		#relatedlist-watermark {
			height: 150px;
