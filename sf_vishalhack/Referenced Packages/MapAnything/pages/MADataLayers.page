        <script>
            $(function() {
            
                makeComboboxes();   
                         
            });
            
            function makeComboboxes()
            {
                //reset all fields to first option
                var comboBoxIndex = $("#fieldSelection .comboBox").length;
                while (comboBoxIndex--) {
                    var $combo = $("#fieldSelection .comboBox").eq(comboBoxIndex);
                    $combo.find("option:first").attr('selected','selected');
                }
                
                //parse the data if we have some
                parseJsonDataLayers();
                
                $('select.comboBox').combobox();
                
                //remove salesforce button styles
                $('.MAbutton').removeClass('btn btnDisabled');
            }
            
            function parseJsonDataLayers () {
                if($('[id$=":JsonData"]').val() != '' && $('[id$=":JsonData"]').val() != undefined) {
                    
                    try {
                        var jsonData = JSON.parse($('[id$=":JsonData"]').val());
                        
                        //update selection 
                        var objectSelection = $('#objectSelection .comboBox').val();
                        
                        //get c2c
                        var c2cSettings = jsonData['C2C'];
                        
                        //loop over the selection and update the values
                        var objectData = c2cSettings[objectSelection];
                        var fields = objectData['fields'];
                        for (key in fields) {
                            //find the selectbox and update the value
                            $('#fieldSelection .comboBox[data-label="'+key+'"]').val(fields[key]);
                        }
                        var fieldSet = objectData['fieldSetOptions'];
                        for (i=0;i<fieldSet.length;i++) {
                            var fset = fieldSet[i];
                            //find the selectbox and update the value
                            $('#fieldSetOptions .comboBox[data-recordTypeId="'+fset.RecordTypeId+'"]').val(fset.FieldSetAPIName);
                        }
                    }
                    catch (e) {
                        console.log(e);
                    }
                }
                
            }
            
            function buildJSONSaveData() {
                //get the table
                var $table = $('#fieldSelection');
                var $selections = $table.find('.comboBox');
                
                //get the object we are saving
                var objectName = $('#objectSelection .comboBox').val();
                
                //loop over the selections and build json
                var jsonData = {};
                for(i=0; i<$selections.length; i++) {
                    var $select = $selections.eq(i);
                    
                    if($select.val() != '--') {
                        //get the select value and data-value and store it
                        jsonData[$select.attr('data-label')] = $select.val();
                    }
                }
                
                //loop over field sets and save data
                var $fieldSetTable = $('#fieldSetOptions .FieldMappingTable');
                var $fieldSetSelections = $fieldSetTable.find('.comboBox');
                
                var fieldSetArray = [];
                for(p=0; p<$fieldSetSelections.length; p++) {
                    var fsJsonData = {};
                    var $select2 = $fieldSetSelections.eq(p);
                    
                    //get the select value and data-value and store it
                    fsJsonData = {
                        IsSystemDefault : $select2.attr('data-systemDefault'),
                        FieldSetAPIName : $select2.val(),
                        RecordTypeName : $select2.attr('data-label'),
                        RecordTypeId : $select2.attr('data-recordTypeId')
                    }
                    fieldSetArray.push(fsJsonData);
                }
                
                //combine with previous save data
                var origJsonData = {};
                var dataLayerId;
                var c2cSettings = {};
                if($('[id$=":JsonData"]').val() != '') {
                    dataLayerId = $('[id$=":DataLayerId"]').val();
                    origJsonData = JSON.parse($('[id$=":JsonData"]').val());
                    
                    c2cSettings = origJsonData['C2C'];
                }
                
                //get record types
                var recordTypeRows = $('.recordTypeRow');
                var recordTypeArr = [];
                for(var i = 0; i < recordTypeRows.length; i++) {
                    var $row = recordTypeRows.eq(i);
                    var rtObj = {
                        label : $row.find('.recordTypeLabel').text(),
                        id : $row.attr('recordtypeid')
                    }
                    
                    recordTypeArr.push(rtObj);
                }
                
                //overwrite the c2c settings with the new
                c2cSettings[objectName] = {
                    objAPIName : objectName,
                    objLabel : $('#objectSelection .comboBox option:selected').text(),
                    fields : jsonData,
                    fieldSetOptions : fieldSetArray,
                    recordTypes : recordTypeArr
                }
                
                //wrap it all back up
                origJsonData['C2C'] = c2cSettings;
                
                //grab all the data
                var serializedDataLayer = JSON.stringify({
                        Id : dataLayerId,
                        sma__Type__c : $('#dataLayerSettings .layerSelection').val(),
                        sma__Settings__c : JSON.stringify(origJsonData)
                });
                
                //remote action to save
                try{
                    (typeof sma == 'undefined' ? MADataLayers : sma.MADataLayers).doSave(serializedDataLayer, function (res) {
                        if(res.success) {
                            //show success popup
                            var popup = MAPopup.showMAPopup({
                				template: 'The settings have been updated.',
                				width : 250,
                				title: 'Success',
                				buttons: [
                					{ 
                						text: 'Ok',
                						type: 'button-blue',
                					}
                				]
                			});
                			
                			$('[id$=":JsonData"]').val(res.data.sma__Settings__c);
                			
                			//hide the popup after 3 secs
                			setTimeout(function () {
                			    popup.close();
                			}, 3000);
                        }
                        else {
                            var popup = MAPopup.showMAPopup({
                				template: '<div>The settings were unable to be updated.<div></br><div>'+res.data+'</div>',
                				width : 250,
                				title: 'Error',
                				buttons: [
                					{ 
                						text: 'Ok',
                						type: 'button-red',
                					}
                				]
                			});
                        }
                    },
                    {escape: false});
                }
                catch (e) {
                    
                }
            }
            
            //CUSTOM POPUP FRAMEWORK
            var MAPopup = {
    			currentPopup : null,
    	        HTML: 
    	            '<div class="maPopup-container">'+
    	                '<div class="maPopup">'+
    	                    '<div class="maPopup-head">'+
    	                        '<h3 class="maPopup-title">Alert</h3>'+
    	                        '<div class="maPopup-subtitle"></div>'+
    	                    '</div>'+
    	                    '<div class="maPopup-body"></div>'+
    	                   '<div class="maPopup-footer">'+
    	                    '</div>'+
    	                '</div>'+
    	            '</div>',
    	        blankFunc : function(){},
    
    	        maPopupButtonClick: function(button) {
    	            var $button = $(button);
    	            var maPopup = $(button).data('maPopup');
    	            button = $button.data('button');
    
    	            
    	            var result = (button.onTap || this.blankFunc)(event);
    	            //return the function
    	            maPopup.deferred.resolve(result);
    	        },
    
    	        createMAPopup : function(options) {
    	        	//check if backdrop exists
    	        	if($('.backdrop').length == 0) {
    	        		$('<div class="backdrop"></div>').appendTo('body');
    	        	}
    
    	            options = $.extend({
    	                title : '',
    	                width : 250,
    	                buttons : []
    	            }, options || {});
    
    	            var popup = {};
    	            popup.element = $(this.HTML);
    	            popup.deferred = $.Deferred();
    
    	            //loop over the buttons
    	            for(var i = 0; i < options.buttons.length; i++) {
    	                var button = options.buttons[i];
    	                
    	                var $button = $('<button class="MAbutton '+button.type+'" onclick="MAPopup.maPopupButtonClick(this)">'+button.text+'</button>').data({'button':button,'maPopup':popup});
    	                popup.element.find('.maPopup-footer').append($button);
    	            }
    
    	            //update the popup with options
    	            popup.element.find('.maPopup').css('width',options.width);
    	            popup.element.find('.maPopup-title').text(options.title || 'Alert');
    	            popup.element.find('.maPopup-subtitle').text(options.subTitle || '');
    	            var popupBody = popup.element.find('.maPopup-body');
    	            if(options.template) {
    	                popupBody.html(options.template);
    	            }
    	            else {
    	                popupBody.remove()
    	            }
    
    	            //append the the body
    	            $('body').append(popup.element);
    
    	            popup.show = function () {
    	                //scroll to top
    	                $("html, body").animate({ scrollTop: 0 }, "fast");
    	                
    	                //do nothing if already visible or removed
    	                if(popup.isShown || popup.removed) return;
    
    	                //set this popup as visible
    	                popup.isShown = true;
    	                popup.element.removeClass('popup-hidden');
    	                popup.element.addClass('maPopup-visible active');
    	                //focus input?
    	            }
    
    	            popup.hide = function (callback) {
    	                callback = callback || this.blankFunc;
    
    	                //if not visible just return the callback
    	                if(!popup.isShown) return callback();
    
    	                popup.isShown = false;
    	                popup.element.removeClass('active');
    	                popup.element.addClass('maPopup-hidden');
    	                callback();
    	            }
    
    	            popup.remove = function () {
    	                //if already remove, return
    	                if(popup.removed) return;
    
    	                this.hide(function() {
    	                    popup.element.remove();
    	                });
    
    	                popup.removed = true;
    	            }
    	            
    	            this.currentPopup = popup;
    	            return popup;
    	        },
    
    	        showMAPopup : function (options) {
    	            //create the popup
    	            var popup = this.createMAPopup(options);
    	            
    	            //show backdrop
    	            $('.backdrop').addClass('active visible');
    	            //$popup.addClass('active');
    
    	            popup.deferred.promise().close = function popupClose(result) {
    	                if (!popup.removed) {
    	                    popup.deferred.resolve(result);
    	                }
    	            }
    
    	            doShow();
    
    	            return popup.deferred.promise();
    
    	            function doShow() {
    	                setTimeout(function() {
    	                    popup.show();
    	                },200);
    
    	                //this will be run when a button is clicked
    	                popup.deferred.promise().then(function(result) {
    	                    //hide the backdrop
    	                    $('.backdrop').removeClass('active visible');
    	                    //remove the popup
    	                    popup.remove();
    
    	                    return result;
    	                });
    
    	            }
    	        },
    
    	        showMAConfirm : function (opts) {
    	            return this.showMAPopup($.extend({
    	              buttons: [{
    	                text: opts.cancelText || 'Cancel',
    	                type: opts.cancelType || 'button-default',
    	                onTap: function() { return false; }
    	              }, {
    	                text: opts.okText || 'OK',
    	                type: opts.okType || 'button-positive',
    	                onTap: function() { return true; }
    	              }]
    	            }, opts || {}));
    	        }
    
    
    	    }
        
        </script>
