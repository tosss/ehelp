.pageForm {
    min-height: 700px; 
    padding: 10px;
}
.searchbox {
    line-height: 1.5em;
    font-size: 1.5em;
    border: solid 1px #aaa;
    width: 300px;
    padding: 0 2px;
}
.ui-tooltip {
    background-color: #fefdb9;
    border: 1px solid orange;
    color: #000;
    border-radius: 0;
    font: bold 12px "Helvetica Neue", Sans-Serif;
}
.tooltip {
    cursor: default;
}

.pageForm .bPageBlock table {
    width: 80%;
}

.pageForm .bPageBlock table th {
    vertical-align: top;
}

.hours, .Mins {
    float: left;
    padding: 20px 20px 10px 0;
}

.date span, .hours span, .Mins span {
    font-size: 0.8em;
    font: bold 0.8em "Courier New";
}

.hours select, .Mins select {
    width: 80px;
    font-size: 1.2em;
}

.actions {
    margin: 30px 0;
}