/*  GO FULL WIDTH AT LESS THAN 480 PIXELS */

@media only screen and (max-width: 480px) {
    .col { 
        margin: 1% 0 1% 0%;
    }
}

@media only screen and (max-width: 480px) {
    .span_3_of_3 {
        width: 98%;
        
    }
    .span_2_of_3 {
        width: 98%;
        
    }
    .span_1_of_3 {
        width: 98%;
        
    }
    
    .span_1_of_2 {
        width: 95%;
        padding:10px;
        
    }
    .span_2_of_2 {
        width: 95%;
        
    }
    
}

@media only screen and (max-width: 480px) {
    #wrapper-body h2 { 
        top:110px;
    }
}   
@media only screen and (max-width: 480px) {
    #wrapper { 
        padding:18px;
    }
}
/* Css hack for IE */
@media all and (-ms-high-contrast:none)
    {
    .section { margin-top: -12px; } /* IE10 */
    #content { margin-top: -12px; }
    *::-ms-backdrop, .section { margin-top: -12px; } /* IE11 */
    *::-ms-backdrop, #content { margin-top: -12px; } /* IE11 */
}