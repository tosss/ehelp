#wrapper-body {
    margin: 0px auto; 
    padding: 0; 
    background-color: #666; 
    max-width: 1080px; 
    margin-top: 16px;
}

#container {
    margin: 0px auto; 
    padding: 0 0 25px 0; 
    background: #fff; 
    border: 0 2px 2px 2px solid #e3e3e3; 
    max-width: 1000px; 
}

/*  NAVIGATION  */

#nav-container {
    width: 960px;
    margin: auto 0;
    min-height: 40px;
    background-color: #000;
}

#nav ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

#nav li {
    float: left;        
}

#nav a {
    display: block;
    width: 137px;
    background-color: #000;
    text-align:center;
    padding:8px 0;
    font-family: "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, "Open Sans", sans-serif;
    font-weight:400;
    font-style:normal;
    font-size:17px;
    color:#fff;
}

#nav a:active {
    background-color: #81bd00;
}

#sub-nav-container {
    width: 960px;
    min-height: 40px;
    background-color: #666;
}

#sub-nav ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

#sub-nav li {
    float: left;
    height:40px;
}

#sub-nav a {
    display: block;
    min-width: 120px;
    height: 24px;
    background-color: #666;
    text-align: center;
    padding: 8px 0;
    font-family: "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, "Open Sans", sans-serif;
    font-weight: 400;
    font-style: normal;
    font-size: 17px;
    color: #fff;
    text-decoration: inherit;
}


/*  SECTIONS  */

#content {
    width: 920px;
    min-height: 250px;
}

#footer {
    max-width: 815px;
    text-align:left;
    color: #fff;
    border: 1px solid #e3e3e3; 
    margin-top: 20px;
    padding: 20px 20px 20px 25px;
    background: #fff;
}

.section {
    clear: both;
    padding: 0px;
    margin: auto 0;
    max-width: 1000px;
    background: url('/Employees/resource/1410547953000/EMP_green_white_bottom');
    background-repeat: repeat-x;
}

.section-banner {
    clear: both;
    padding: 0px;
    margin: -16px 0 0 0;
    max-width: 100%;
    min-height: 276px;
    background-color: #81bd00;
}

.section-banner-sub {
    clear: both;
    padding: 0px;
    margin: -16px 0 0 0;
    max-width: 100%;
    min-height: 140px;
    background-color: #81bd00;
}

.home-content-details {
    margin-top: 10px;
    margin-bottom: 5px;
}

.section-banner-sub-Home-details {
    background-repeat: no-repeat; 
    background-position: 40px center; 
    height: 300px;
}

.section-banner-sub-details {
    background-repeat: no-repeat; 
    background-position: 40px center; 
    height: 200px;
}

.span_Align_Text {
    text-align:left !important;
}

/*  COLUMN SETUP  */

.col {
    display: block;
    margin: 0 0 0 1%;
}

.col:first-child { 
    margin-left: 0; 
}

.col li {
    padding-bottom: 4px;
}

/*  GRID OF THREE  */

.three_col {
    width: 30%;
    float: left;
    min-height: 250px; /* Changed to 250 from 225 by Lalitha Adduru on 23/02/2015 */
    padding-left: 1.333%;
    padding-bottom: 1.333%;
    padding-right: 1.333%;
    margin-bottom: 10px;
}

.span_3_of_3 {
    background-color: #ebebeb;
}

.span_2_of_3 {
    background-color: #e0e0e0;
}

.span_1_of_3 {
    padding-right: 0.1%;     /* Added by Lalitha Adduru on 23/02/2015 */
    padding-top: 0%;		/* Added by Lalitha Adduru on 23/02/2015 */
    background-color: #ccc;
}

.span_combine_2col {
    float: left;  
    min-height: 225px;
    width: 60%;
    text-align: left;
    background-color: #e0e0e0;
}

.span_combine_2col_Content {
    padding: 10px;
}

.span_combine_3col {
    float: left;  
    min-height: 225px;
    text-align: left;
    height: 300px;
    overflow: auto;
    background-color: #e0e0e0;
    width: 97.34%;
    padding: 1.33%;
}

.span_3_of_3_MultiRow {
    float: left;
    min-height: 225px;
    width: 39%;
    text-align: left;    
}

.span_3_of_3_MultiRow_Content:first-child {
    margin-top: 0;
}

.span_3_of_3_MultiRow_Content {
    margin-top: 10px;
    padding: 10px;
    background-color: #ebebeb;
}

.span_combine_2Multi {
	float: left;  
	min-height: 225px;
	width: 60%;
	text-align: left;          
}
.span_combine_2colMulti_Content:first-child {
	margin-top: 0;
}

.span_combine_2colMulti_Content {
	margin-top:10px;
	padding: 10px;
	background-color: #e0e0e0;
	margin-left:0px;
}

.hideComponent {
	display:none !important;
}
/*  GRID OF THREE NO BACKGROUND 3 NARROW COLUMNS NO BACKGROUND  */

.span_nobkg {
    float: left;
    border: none;
    background-color: transparent;
    min-height: 225px;
    padding: 25px 0 0 15px;
    
}

.span_3_of_3_nobkg {
    width: 34%;
