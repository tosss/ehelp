.pageForm {
    min-height: 700px; 
    padding: 10px;
}
.searchbox {
    line-height: 1.5em;
    font-size: 1.5em;
    border: solid 1px #aaa;
    width: 300px;
    padding: 0 2px;
}
.ui-tooltip {
    background-color: #fefdb9;
    border: 1px solid orange;
    color: #000;
    border-radius: 0;
    font: bold 12px "Helvetica Neue", Sans-Serif;
}
.tooltip {
    cursor: default;
}