        <apexClass>ALL_TOTPPlugin</apexClass>
        <connector>
            <targetReference>Registration</targetReference>
        </connector>
        <inputParameters>
            <name>OTP_INPUT</name>
        </inputParameters>
        <inputParameters>
            <name>OTP_REGISTRATION_INPUT</name>
        </inputParameters>
        <inputParameters>
            <name>SECRET_INPUT</name>
        </inputParameters>
        <outputParameters>
            <assignToReference>qr_url</assignToReference>
