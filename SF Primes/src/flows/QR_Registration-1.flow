    <apexPluginCalls>
        <name>Validation</name>
        <label>Validation</label>
        <locationX>476</locationX>
        <locationY>358</locationY>
        <apexClass>ALL_TOTPPlugin</apexClass>
        <connector>
            <targetReference>Login</targetReference>
        </connector>
        <inputParameters>
            <name>OTP_INPUT</name>
        </inputParameters>
        <inputParameters>
            <name>OTP_REGISTRATION_INPUT</name>
            <value>
                <elementReference>InputRegistrationToken</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>SECRET_INPUT</name>
            <value>
                <elementReference>secret</elementReference>
            </value>
        </inputParameters>
        <outputParameters>
            <assignToReference>IsTokenValid</assignToReference>
            <name>IsValid_OUTPUT</name>
        </outputParameters>
    </apexPluginCalls>
    <decisions>
        <name>Login</name>
        <label>Login</label>
        <locationX>259</locationX>
        <locationY>361</locationY>
        <defaultConnector>
            <targetReference>Registration</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Token is invalid</defaultConnectorLabel>
        <rules>
            <name>Token_is_valid</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>IsTokenValid</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Registration_Succesfull</targetReference>
            </connector>
            <label>Token is valid</label>
        </rules>
    </decisions>
    <decisions>
        <name>Registration</name>
        <label>Registration</label>
        <locationX>257</locationX>
        <locationY>225</locationY>
        <defaultConnector>
            <targetReference>Already_Registered</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Already Registered.</defaultConnectorLabel>
        <rules>
            <name>Register</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>secret</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Registration_Screen</targetReference>
            </connector>
            <label>Register</label>
        </rules>
    </decisions>
