    <validationRules>
        <fullName>IsParent_AnswerType_Values</fullName>
        <active>true</active>
        <description>When Is Parent is selected as Yes, only &apos;MultiSelect Picklist,&apos; &apos;Picklist,&apos; &apos;YesNo,&apos; and &apos;Checkbox&apos; can be selected for Answer Type.</description>
        <errorConditionFormula>ISPICKVAL(Is_Parent__c,&quot;Yes&quot;) &amp;&amp; ( ISPICKVAL( Answer_Type__c , &quot;Currency&quot; ) || ISPICKVAL( Answer_Type__c , &quot;Date&quot;) || ISPICKVAL( Answer_Type__c , &quot;Number&quot;) || ISPICKVAL( Answer_Type__c , &quot;Text&quot;) || ISPICKVAL( Answer_Type__c , &quot;URL&quot;) ) &amp;&amp; NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c))</errorConditionFormula>
        <errorDisplayField>Answer_Type__c</errorDisplayField>
        <errorMessage>Only &apos;MultiSelect Picklist,&apos; &apos;Picklist,&apos; &apos;YesNo,&apos; and &apos;Checkbox&apos; can be selected for Answer Type when Is Parent is &apos;Yes&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IsParent_ToggleDependent_Required</fullName>
        <active>true</active>
        <description>When Is Parent is &apos;Yes&apos;, then a value needs to be specified for Toggle Dependent.</description>
        <errorConditionFormula>ISPICKVAL(Is_Parent__c,&quot;Yes&quot;) &amp;&amp; ISBLANK(Toggle_Dependent__c) &amp;&amp; NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c))</errorConditionFormula>
        <errorDisplayField>Toggle_Dependent__c</errorDisplayField>
        <errorMessage>Please enter the Toggle value related to the Answer Type.</errorMessage>
    </validationRules>
