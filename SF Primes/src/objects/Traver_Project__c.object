        <fullName>End_of_Term_Date_PSG__c</fullName>
        <externalId>false</externalId>
        <formula>DATE (  
/*YEAR*/  
YEAR(Install_Date_PSG__c) + FLOOR((MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c - 1)/12),
/*MONTH*/  
CASE(MOD(MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c, 12 ), 0, 12, MOD(MONTH(Install_Date_PSG__c)+ Contract_Term_PSG__c, 12 )),  
/*DAY*/  
MIN(DAY(Install_Date_PSG__c),  CASE(MOD(MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c,12), 9, 30, 4, 30, 6, 30, 11, 30, 2,  
/* return max days for February dependent on if end date is leap year */  
IF(MOD(YEAR(Install_Date_PSG__c) + FLOOR((MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c)/12), 400) = 0 || (MOD(YEAR(Install_Date_PSG__c) + FLOOR((MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c)/12), 4) = 0 
&amp;&amp; MOD(YEAR(Install_Date_PSG__c) + FLOOR((MONTH(Install_Date_PSG__c) + Contract_Term_PSG__c)/12), 100) &lt;&gt; 0  ), 29,28), 31))  )</formula>
