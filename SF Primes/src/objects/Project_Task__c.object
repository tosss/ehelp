    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Actual_End_Date_48Hrs_After_Plan</fullName>
        <active>true</active>
        <description>Reason field must not be null if Actual End Date is 48 hours after Planned End Date</description>
        <errorConditionFormula>AND(
	NOT(
		AND(
			CONTAINS(
				 $Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c,  $User.FirstName + &apos; &apos; + $User.LastName
				 ),
				 
		    $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;
		    )
		), 
	(
	Actual_End_Date__c &gt;  (Planned_End_Date__c + 2)
	) &amp;&amp;   ISPICKVAL(Reason_Missed_Planned_End_Date__c, &quot;&quot;),(NOT(ISCHANGED(Status__c))), Status__c = &apos;Complete&apos;
	)</errorConditionFormula>
        <errorDisplayField>Reason_Missed_Planned_End_Date__c</errorDisplayField>
        <errorMessage>Reason field must not be null if Actual End Date is 48 hours after Planned End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_End_Date_Before_Actual_Time_Spent</fullName>
        <active>true</active>
        <description>Actual Time Spent should have a value before entering an Actual End Date value.</description>
        <errorConditionFormula>AND(
	NOT(
		AND(
			CONTAINS(
				 $Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c,  $User.FirstName + &apos; &apos; + $User.LastName
				 ),
				 
		    $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;
		    )
				 
		),
	
	
	ISBLANK(Actual_Time_Spent__c) &amp;&amp; (!ISBLANK(Actual_End_Date__c)) &amp;&amp; Status_Not_Applicable__c = false
	)</errorConditionFormula>
        <errorMessage>Actual End Date cannot be entered before you enter a value for Actual Days Spent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_End_Date_Before_Start</fullName>
        <active>true</active>
        <description>Actual End Date cannot occur before Actual Start Date.</description>
        <errorConditionFormula>IF(( NOT(ISBLANK(Actual_End_Date__c)) &amp;&amp; NOT(ISBLANK(Actual_Start_Date__c)) ), ( Actual_End_Date__c &lt; Actual_Start_Date__c ), FALSE)</errorConditionFormula>
        <errorMessage>Actual End Date cannot occur before Actual Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Time_Spent_Before_Actual_Start_Da</fullName>
        <active>true</active>
        <description>Actual Start Date should have a value before entering an Actual Time Spent.</description>
        <errorConditionFormula>AND(
	NOT(
		AND(
			CONTAINS(
				 $Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c,  $User.FirstName + &apos; &apos; + $User.LastName
				 ),
				 
		    $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;
		    )
		), 
	ISBLANK(Actual_Start_Date__c) &amp;&amp; (Actual_Time_Spent__c != 0 || !ISBLANK(Actual_Time_Spent__c)) &amp;&amp; Status_Not_Applicable__c = false
	)</errorConditionFormula>
        <errorDisplayField>Actual_Time_Spent__c</errorDisplayField>
        <errorMessage>Actual Days Spent cannot be entered before you enter a value for Actual Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Time_Spent_Valid_Range</fullName>
        <active>true</active>
        <description>Actual Days Spent cannot be less than 0, and cannot match or exceed 100 days (i.e. it must be less than 100 days).</description>
        <errorConditionFormula>OR( Actual_Time_Spent__c &lt; 0, Actual_Time_Spent__c &gt;= 100)</errorConditionFormula>
        <errorDisplayField>Actual_Time_Spent__c</errorDisplayField>
        <errorMessage>Actual Days Spent cannot be less than 0, and cannot match or exceed 100 days (i.e. it must be less than 100 days).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assign_Associate_before_Actual_Dates</fullName>
        <active>true</active>
        <description>An associate (CDK Assignee) needs to be entered prior to inputting Actual Dates.</description>
        <errorConditionFormula>AND(NOT(CONTAINS($Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c, $User.FirstName + &apos; &apos; + $User.LastName)),ISBLANK(CDK_Assignee__c) &amp;&amp; NOT(ISBLANK(Actual_Start_Date__c)),
OR($Profile.Name != &apos;ADPDS - Admin - Integration User&apos;, $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;))</errorConditionFormula>
        <errorMessage>A CDK Assignee needs to be filled-in prior to entering Actual Start or End Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assign_Associate_before_Actual_Dates_PA</fullName>
        <active>true</active>
        <description>An associate (CDK Assignee) needs to be entered prior to inputting Actual Dates at the Project Activity</description>
        <errorConditionFormula>AND(NOT(CONTAINS($Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c, $User.FirstName + &apos; &apos; + $User.LastName)), ISBLANK(Project_Activity__r.CDK_Assignee__c) &amp;&amp; NOT( ISBLANK(Actual_Start_Date__c)),$Profile.Name != &apos;ADPDS - Admin - Integration User&apos; || $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;)</errorConditionFormula>
        <errorMessage>A CDK Assignee needs to be filled-in prior to entering Actual Start or End Dates at the Project Activity level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Create_Task_if_Activity_Complete</fullName>
        <active>true</active>
        <description>Cannot Create Project Task Record if Project Activity is Complete</description>
        <errorConditionFormula>AND (
        NOT( CONTAINS($Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c, $User.FirstName + &apos; &apos; + $User.LastName) ),
        $Profile.Name != &apos;ADPDS - Admin - Integration User&apos; ||$Profile.Name != &apos;Cobalt Cast Iron Integration&apos;,
        Project_Activity__r.Status__c = &apos;Complete&apos;, ISNEW()
    )</errorConditionFormula>
        <errorMessage>You cannot create a Project Task record if its parent Project Activity is already Complete.  Please remove the record that you’re trying to create.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Referenced_Definition</fullName>
        <active>true</active>
        <description>An assigned definition cannot be modified.</description>
        <errorConditionFormula>PRIORVALUE(Task_Definition__c ) != null &amp;&amp; ISCHANGED(Task_Definition__c )</errorConditionFormula>
        <errorMessage>You cannot modify an existing record&apos;s referenced definition.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_before_Start_Date</fullName>
        <active>true</active>
        <description>Cannot save a record with Planned/Actual End Date before setting Planned/Actual Start Date.</description>
        <errorConditionFormula>( ISBLANK(Actual_Start_Date__c) &amp;&amp; NOT( ISBLANK(Actual_End_Date__c)) ) || ( ISBLANK(Planned_Start_Date__c) &amp;&amp; NOT( ISBLANK(Planned_End_Date__c)) )</errorConditionFormula>
        <errorMessage>Planned / Actual Start Date needs to be specified before specifying a Planned / Actual End Date for the Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_Time_Spent_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	NOT(
		AND(
			CONTAINS(
				 $Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c,  $User.FirstName + &apos; &apos; + $User.LastName
				 ),
				 
		    $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;
		    )
				
	    ),
	 
	OR (
		ISBLANK(Expected_Time_Spent__c ), 
		Expected_Time_Spent__c &lt; 0)
	)</errorConditionFormula>
        <errorMessage>Expected Days Spent is Required!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Future_Actual_Start_End_Dates</fullName>
        <active>true</active>
        <description>Does not allow Actual Start Date or Actual End Date to be in the future.</description>
        <errorConditionFormula>( Actual_Start_Date__c &gt; TODAY() ) || ( Actual_End_Date__c &gt; TODAY() )</errorConditionFormula>
        <errorMessage>Actual Start and End Dates cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planned_End_Date_Before_Start</fullName>
        <active>true</active>
        <description>Planned End Date cannot occur before Planned Start Date.</description>
        <errorConditionFormula>IF( ( NOT(ISBLANK(Planned_End_Date__c)) &amp;&amp; NOT(ISBLANK(Planned_Start_Date__c)) ),( Planned_End_Date__c &lt; Planned_Start_Date__c ), FALSE )</errorConditionFormula>
        <errorDisplayField>Task_Definition__c</errorDisplayField>
        <errorMessage>Planned End Date cannot occur before Planned Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Edit_RMSRecord_Task</fullName>
        <active>true</active>
        <description>Prevent specified fields for RMS-Owned Records (i.e. where RMS_Owned_Flag__c = true) from being updated unless updated by Integration User or a System Administrator</description>
        <errorConditionFormula>AND 
( 
	RMS_Owned_Flag__c == true , 
	OR(
		ISCHANGED( Description__c ),
		ISCHANGED( CDK_Assignee__c ), 
		ISCHANGED( Planned_Start_Date__c ), 
		ISCHANGED( Planned_End_Date__c ) ,
		ISCHANGED( Expected_Time_Spent__c ) ,
		ISCHANGED( Actual_Start_Date__c ) ,
		ISCHANGED( Actual_End_Date__c ) ,
		ISCHANGED( Actual_Time_Spent__c ) ,
		ISCHANGED( Migration_Status__c ) ,
		ISCHANGED( Reason_Missed_Planned_End_Date__c ),
                ISCHANGED( Reason__c ) 		 		
	), 
	NOT($Profile.Name == &apos;ADPDS - Admin - System Administrator&apos;), 
	NOT($Profile.Name == &apos;ADPDS - Admin - Integration User&apos;),
        NOT($Profile.Name == &apos;Cobalt Cast Iron Integration&apos;)
)</errorConditionFormula>
        <errorMessage>This record is owned by RMS. Contact an RMS scheduler to change the CDK Assignee.  Other updates should be performed by the owner of the task via the My RMS Assignments tab in CDK Expressway.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Not_Applicable</fullName>
        <active>true</active>
        <description>Reason field must have a value if status is Not Applicable</description>
        <errorConditionFormula>Status__c != &quot;&quot; &amp;&amp; Status__c = &quot;Not Applicable&quot; &amp;&amp; ISPICKVAL( Reason__c , &quot;&quot;) &amp;&amp; (ISCHANGED(Status__c))</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Reason cannot be null if status is Not Applicable.</errorMessage>
    </validationRules>
</CustomObject>
