    <validationRules>
        <fullName>IMP_CMF_8Char</fullName>
        <active>true</active>
        <description>Require that the CMF field be a minimum of 8 characters</description>
        <errorConditionFormula>AND( RecordType.Name  = &quot;Non SubProject Related&quot;,OR(LEN( Dealership_CMF__c ) &lt;&gt; 8),NOT(ISBLANK(Dealership_CMF__c)))</errorConditionFormula>
        <errorDisplayField>Dealership_CMF__c</errorDisplayField>
        <errorMessage>The CMF must be numeric and contain 8 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IMP_Other_Product_Name_Required</fullName>
        <active>true</active>
        <description>Validation to enforce entry of a product name if Other is selected from the Product Installed picklist</description>
        <errorConditionFormula>AND(INCLUDES( Products_Installed__c , &quot;Other&quot;),ISBLANK( Other_Product_Names__c))</errorConditionFormula>
        <errorDisplayField>Other_Product_Names__c</errorDisplayField>
        <errorMessage>You selected &quot;other&quot; as a product installed. Please enter the name(s) for the product(s) you installed.....[IMP_Other_Product_Name_Required]</errorMessage>
    </validationRules>
