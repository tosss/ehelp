    <fields>
        <fullName>Dealer_Group__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;/&quot;+ 
IF 
( 
ISBLANK( Account__r.Corporation__c ), 
IF 
( 
ISBLANK( Account__r.Group__c ), 
Account__c, 
Account__r.Group__c 
), 
Account__r.Corporation__c 
), 
IF 
( 
ISBLANK( Account__r.Corporation__c ), 
IF 
( 
ISBLANK( Account__r.Group__c ), 
Account__r.Name, 
Account__r.Group__r.Name 
), 
Account__r.Corporation__r.Name 
) 

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Dealer Group</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
