            INCLUDES(User_Groups_Impacted__c, &quot;CB - Sales&quot;),
            INCLUDES(User_Groups_Impacted__c, &quot;CB - Mktg&quot;),
            INCLUDES(User_Groups_Impacted__c, &quot;DS - CrdtSvcs&quot;),
