        <externalId>false</externalId>
        <formula>IF(
ISPICKVAL( Status__c , &apos;Completed&apos;) || ISPICKVAL( Status__c , &apos;Cancelled - Not Approved&apos;) || ISPICKVAL( Status__c , &apos;Cancelled - Duplicate&apos;) || ISPICKVAL( Status__c , &apos;Cancelled - BU Declined&apos;), &quot;&quot;, IF(
LOWER(Owner:Queue.DeveloperName) &lt;&gt; &quot;requestqueue&quot; &amp;&amp;
NOT(ISBLANK(All_Request_Team_Members__c)) &amp;&amp;
(SF_Config_Needed__c = False || NOT(ISBLANK( SF_Config_Admins__c ))) &amp;&amp;
(SF_Dev_Needed__c = False || NOT(ISBLANK( SF_Developers__c ))) &amp;&amp;
NOT(ISPICKVAL(Change_Classification__c,&quot;&quot;)) &amp;&amp;
(Completed_in_QA__c = True &amp;&amp; Completed_in_Unified__c = True) &amp;&amp;
NOT(ISNULL(Duration_to_Completion__c)) &amp;&amp;
NOT(ISBLANK(Initial_Estimate__c)) &amp;&amp;
NOT(ISPICKVAL(Review_Status__c , &quot;&quot;) &amp;&amp;
LOWER( RecordType.Name ) = &quot;salesforce feature&quot;), 
&quot;True&quot;, 
&quot;False&quot;))</formula>
        <label>Ready To Complete</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
