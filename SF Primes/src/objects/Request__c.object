    <validationRules>
        <fullName>Do_not_close_Request_if_owned_by_Queue</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LOWER(Owner:Queue.DeveloperName) = &quot;requestqueue&quot;, 
ISCHANGED( Status__c ),  
ISPICKVAL( Status__c , &apos;Completed&apos;) )
&amp;&amp;
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>You cannot close a Request if it is owned by the Request Queue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IT_Do_Not_Complete_if_Review_Not_Done</fullName>
        <active>true</active>
        <description>If Review Required is checked and review status is blank and Satus = Completed - prevent save</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Completed&quot;) &amp;&amp;
PRIORVALUE( Status__c ) &lt;&gt; &quot;Completed&quot; &amp;&amp;
NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)) &amp;&amp;
/* Review_Required__c = True &amp;&amp;  */
CASE( Review_Status__c , &quot;Review Pass&quot;, &quot;Review Pass&quot;, &quot;Review Comments&quot;, &quot;Review Comments&quot;, &quot;&quot;) = &quot;&quot; &amp;&amp;
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorDisplayField>Review_Status__c</errorDisplayField>
        <errorMessage>Request requires a review to be completed!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IT_No_Complete_Missing_Critical_Closure</fullName>
        <active>true</active>
        <description>If Review Status = &quot;Review Comments&quot; &amp; Critical Items is not blank and Critical Items Fixed is blank &amp; Satus = Completed - prevent save</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Completed&quot;) &amp;&amp;
PRIORVALUE( Status__c ) &lt;&gt; &quot;Completed&quot; &amp;&amp;
NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)) &amp;&amp;
NOT(ISBLANK(Critical_Items__c)) &amp;&amp;
 ISBLANK(Critical_Items_Fixed_Reviewed__c) &amp;&amp;
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorDisplayField>Critical_Items_Fixed_Reviewed__c</errorDisplayField>
        <errorMessage>Critical ltems need to be fixed to complete request!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IT_No_Complete_Missing_Review_Comments</fullName>
        <active>true</active>
        <description>If Review Status = &quot;Review Comments&quot; and Review Comments is blank - prevent save</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Completed&quot;) &amp;&amp;
PRIORVALUE(Status__c) &lt;&gt; &quot;Completed&quot; &amp;&amp;
NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)) &amp;&amp;
ISPICKVAL(Review_Status__c, &quot;Review Complete&quot;) &amp;&amp; ISBLANK( General_Review_Comments__c) &amp;&amp;
LOWER(RecordType.Name) = &quot;salesforce feature&quot;</errorConditionFormula>
        <errorDisplayField>General_Review_Comments__c</errorDisplayField>
        <errorMessage>Review Comments Required to be completed!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQST_AllRequestTeamMembers_Req</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c))
&amp;&amp; 
ISPICKVAL(Status__c, &apos;Completed&apos;) 
&amp;&amp; 
ISBLANK(All_Request_Team_Members__c) 
&amp;&amp; 
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>Please enter Request Team Members</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQST_AssignedConfig_Required</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))
&amp;&amp;
OR(ISPICKVAL(Status__c, &apos;Completed&apos;), ISPICKVAL(Status__c, &apos;Closed&apos;))
&amp;&amp; 
SF_Config_Needed__c=true 
&amp;&amp; 
ISBLANK( SF_Config_Admins__c ) 
&amp;&amp; 
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>Please add a SF Config Admin who worked on the Request to the Request Team Members</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQST_AssignedDev_Required</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))
&amp;&amp;
OR(ISPICKVAL(Status__c, &apos;Completed&apos;), ISPICKVAL(Status__c, &apos;Closed&apos;))  
&amp;&amp; 
SF_Dev_Needed__c=true 
&amp;&amp; 
ISBLANK( SF_Developers__c ) 
&amp;&amp; 
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>Please add a SF Developer who worked on the Request to the Request Team Members</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQST_Change_Classification_required</fullName>
        <active>true</active>
        <description>Validation rule to enforce users to fill out Change Classification</description>
        <errorConditionFormula>NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;))
&amp;&amp;
OR(ISPICKVAL(Status__c, &apos;Completed&apos;), ISPICKVAL(Status__c, &apos;Closed&apos;)) 
&amp;&amp;
ispickval(Change_Classification__c,&apos;&apos;) 
&amp;&amp;
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>Please select a Change Classification value and Duration to Completion value before closing Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RQST_Chgs_All_Env_Or_Reason_Why_Not</fullName>
        <active>true</active>
        <description>Validation rule to enforce users to check that changes are made in all environments, or put notes in reason why not field.</description>
        <errorConditionFormula>AND(
NOT(BEGINS($User.Username,&quot;integration_user@adp.com&quot;)), 
OR(
  Completed_in_FullDev__c  =  False, 
  Completed_in_Training__c  = False, 
  Completed_in_Prod__c = False),
  ISBLANK( Reason_Not_Completed_In_All_Environments__c ), 
  ISPICKVAL(Status__c, &apos;Closed&apos;),
  ISPICKVAL(Status__c, &apos;Completed&apos;)
)
&amp;&amp;
(LOWER( RecordType.Name ) = &quot;salesforce project&quot; || 
 LOWER( RecordType.Name ) = &quot;salesforce feature&quot;)</errorConditionFormula>
        <errorMessage>A reason for not completing in all environments is required. Please enter the reason why you did not create in all environments.</errorMessage>
    </validationRules>
    <validationRules>
