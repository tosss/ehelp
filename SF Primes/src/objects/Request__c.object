    <fields>
        <fullName>Aged_Days__c</fullName>
        <description>If the status is Open, Need More Information, or Waiting on Approval then calculate from today minus creation date.  If status is Closed  then calculate from last modified date minus creation date.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL( Status__c , &quot;Closed&quot;), ROUND( Date_Time_Closed__c - CreatedDate,0 ),
	IF(ISPICKVAL( Status__c , &quot;Cancelled&quot;), ROUND( LastModifiedDate - CreatedDate,0),
		ROUND(NOW()- CreatedDate,0)	
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Aged Days</label>
        <precision>18</precision>
        <required>false</required>
