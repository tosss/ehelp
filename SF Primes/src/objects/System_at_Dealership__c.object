    <validationRules>
        <fullName>Box_Location_Required</fullName>
        <active>false</active>
        <description>If Box Onsite is checked, then Box Location is required.</description>
        <errorConditionFormula>IF(NOT(Box_Onsite__c ) , LEN(Box_Location__c)=0 , false)</errorConditionFormula>
        <errorDisplayField>Box_Location__c</errorDisplayField>
        <errorMessage>Box Location is Required when the box is not onsite.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Composite_ID_field_required</fullName>
        <active>true</active>
        <description>Composite id field required</description>
        <errorConditionFormula>ISNULL(CompositeExtID__c)||ISBLANK(CompositeExtID__c)</errorConditionFormula>
        <errorDisplayField>CompositeExtID__c</errorDisplayField>
        <errorMessage>Composite external id cannot be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Install_Date_Validation_for_Year</fullName>
        <active>true</active>
        <description>Install Date Year must be between 1970 and 2100</description>
        <errorConditionFormula>NOT ISNULL(Install_Date__c) &amp;&amp; 
(
    YEAR(Install_Date__c) &lt; 1970 || 
    YEAR(Install_Date__c) &gt; 2100
)</errorConditionFormula>
        <errorDisplayField>Install_Date__c</errorDisplayField>
        <errorMessage>Install Date year must be between 1970 and 2100</errorMessage>
    </validationRules>
