    <webLinks>
        <fullName>Create_Deal</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Deal</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js&apos;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/30.0/connection.js&apos;)}

var AccId = &quot;{!Opportunity.AccountId}&quot;; 
var acc = sforce.connection.query(&quot;SELECT id, region__c &quot; + &quot;FROM account WHERE id =&quot; + &quot;&apos;&quot; + AccId + &quot;&apos;&quot;); 
var records = acc.getArray(&apos;records&apos;);
var accRecord = records[0];


var oppId = &quot;{!Opportunity.Id}&quot;;
var opp = sforce.connection.query(&quot;SELECT id, Opportunity__c, Finalized_Deal__c &quot; + &quot;FROM Opportunity_Quotes__c WHERE Opportunity__c =&quot; + &quot;&apos;&quot; + oppId + &quot;&apos;&quot;);
var oppRecords = opp.getArray(&apos;records&apos;);
var opportunityRecord =  oppRecords[0]; 

if(opportunityRecord &amp;&amp; opportunityRecord.Finalized_Deal__c) 
{ 
alert(&quot;A new Deal cannot be created if the Deal State is already Finalized.&quot;); 
} 
else if(accRecord &amp;&amp; accRecord.Region__c == &quot;Division&quot;) 
{ 
alert(&quot;A new Deal cannot be created for Account Region Division.&quot;); 
} 
else 
{ 
window.open(&apos;http://{!$Setup.CustomSettings__c.SQTServer__c}home.aspx?OppID={!Opportunity.Id}&amp;UID={!$User.Id}&apos;, &apos;sqtnew&apos;, &apos;top=150,left=250,height=400,width=400,status=yes,toolbar=no,location=no,menubar=no,scrollbars=yes,resizable=yes&apos;); 
}</url>
    </webLinks>
