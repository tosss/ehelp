    <validationRules>
        <fullName>CB_SVC_AMAAP_Lease_Offer_Type</fullName>
        <active>true</active>
        <description>When the Offer Type is Lease these fields cannot be blank Offer Years - Opt Out, Offer Type - Opt Out, Offer Editor ID - Opt Out, Start - Opt Out and End Date - Opt Out.</description>
        <errorConditionFormula>NOT(Begins($User.Username, $Setup.GlobalSettings__c.Admin_Integration_Username__c)) 
&amp;&amp; 
NOT($Profile.Name=&quot;Cobalt Service Data Exporting&quot;) 
&amp;&amp; 
AND( 
OR(ISPICKVAL(Offer_Types__c, &quot;Lease&quot;), 
ISPICKVAL(Offer_Types__c, &quot;Lease Conquest&quot;), 
ISPICKVAL(Offer_Types__c, &quot;Lease Loyalty&quot;) 
),OR( 
ISBLANK(Offer_Years_Opt_Out__c ), 
ISPICKVAL(Offer_Type_Opt_Out__c, &quot;&quot;), 
ISBLANK(Offer_Editor_ID_Opt_Out__c ), 
ISBLANK(Start_Date_Opt_Out__c ), 
ISBLANK(End_Date_Opt_Out__c ) 
) 
)</errorConditionFormula>
        <errorMessage>When the Offer Type is Lease these fields cannot be blank Offer Years - Opt Out, Offer Type - Opt Out, Offer Editor ID - Opt Out, Start - Opt Out and End Date - Opt Out.</errorMessage>
    </validationRules>
