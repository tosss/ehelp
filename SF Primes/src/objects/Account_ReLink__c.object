        <fullName>Overall_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
    Total_Detail_Records__c = Detail_Successes__c,&quot;Complete&quot;,
    IF(
        Total_Detail_Records__c = Details_Pending__c,&quot;Unprocessed&quot;,
        IF(
            Detail_Errors__c &gt; 0,TEXT(Detail_Errors__c)&amp;&quot; Errors&quot;,
            IF(
                Details_Pending__c &gt; 0,TEXT(Details_Pending__c)&amp;&quot; Pending&quot;,&quot;Unknown&quot;
