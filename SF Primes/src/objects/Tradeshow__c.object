    <validationRules>
        <fullName>DS_Tradeshow_CloseDate_Reqd_For_Ops</fullName>
        <active>true</active>
        <description>If Opportunity_Record_Type is set, require Opportunity Close Date value.</description>
        <errorConditionFormula>NOT(ISBLANK(TEXT(Opportunity_Record_Type__c))) &amp;&amp;
ISBLANK(TEXT(Opportunity_Close_Date__c))</errorConditionFormula>
        <errorDisplayField>Opportunity_Close_Date__c</errorDisplayField>
        <errorMessage>If Opportunity Record Type is set, Opportunity Close Date must be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_Lead_Opp_Not_Both_Set</fullName>
        <active>true</active>
        <description>Ensure Lead Record Type and Opportunity Record Type are not set for a single record.</description>
        <errorConditionFormula>NOT(ISBLANK(TEXT(Lead_Record_Type__c))) &amp;&amp; 
NOT(ISBLANK(TEXT(Opportunity_Record_Type__c)))</errorConditionFormula>
        <errorDisplayField>Opportunity_Record_Type__c</errorDisplayField>
        <errorMessage>Both Lead Record Type and Opportunity Record Type cannot be set for a single record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_Lead_Opp_Set_If_RecType_Set</fullName>
        <active>true</active>
        <description>Ensure Lead-Opp Owner Source is defined if either Lead Rec Type or Opp Rec Type are set OR ensure Lead Rec Type or Opp Rec Type are set if Lead-Opp Owner Source is set.</description>
        <errorConditionFormula>((NOT(ISBLANK(TEXT(Lead_Record_Type__c))) || 
NOT(ISBLANK(TEXT(Opportunity_Record_Type__c))) 
) &amp;&amp; 
ISBLANK(TEXT(Lead_Opp_Owner_Source__c)) 
) 
|| 
(ISBLANK(TEXT(Lead_Record_Type__c)) &amp;&amp; 
ISBLANK(TEXT(Opportunity_Record_Type__c)) &amp;&amp; 
NOT(ISBLANK(TEXT(Lead_Opp_Owner_Source__c))) 
)</errorConditionFormula>
        <errorDisplayField>Lead_Opp_Owner_Source__c</errorDisplayField>
        <errorMessage>Lead-Opp Owner Source needs to be defined if either Lead Record Type or Opportunity Record Type are set OR set Lead Record Type or Opportunity Record Type are set if Lead-Opp Owner Source is set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_NewLead_Lead_Src_Needed</fullName>
        <active>true</active>
        <description>If Lead Record Type is set, ensure Lead Source Value is set.</description>
        <errorConditionFormula>NOT(ISBLANK(TEXT( Lead_Record_Type__c ))) &amp;&amp; 
(ISBLANK(TEXT( Lead_Source_Value__c )))</errorConditionFormula>
        <errorDisplayField>Lead_Source_Value__c</errorDisplayField>
        <errorMessage>Lead Source Value must be set if setting Lead Record Type is set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_OppLead_Status_Reqd</fullName>
        <active>true</active>
        <description>If Opportunity Record Type or Lead Record Type is set, require Opp-Lead Status to have a value.</description>
        <errorConditionFormula>(NOT(ISBLANK(TEXT(Opportunity_Record_Type__c))) ||
 NOT(ISBLANK(TEXT(Lead_Record_Type__c))) 
) &amp;&amp; 
ISBLANK(TEXT(Lead_Opp_Creation_Status__c))</errorConditionFormula>
        <errorDisplayField>Lead_Opp_Creation_Status__c</errorDisplayField>
        <errorMessage>If Opportunity or Lead Record Type is set, Opp-Lead Status must be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_Opp_Lead_Src_Set_Ld_Src</fullName>
        <active>true</active>
        <description>If Opportunity Lead Source Field is set, ensure Lead Source Value is set.</description>
        <errorConditionFormula>NOT(ISBLANK(TEXT( Opportunity_Lead_Source_Field__c ))) &amp;&amp; 
(ISBLANK(TEXT( Lead_Source_Value__c )))</errorConditionFormula>
        <errorDisplayField>Lead_Source_Value__c</errorDisplayField>
        <errorMessage>Lead Source Value must be set if setting Opportunity Lead Source Field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Tradeshow_Party_Tix_No_Other_Fields</fullName>
        <active>true</active>
        <description>If Party Tickets checked, then ensure Lead-Opp Owner Source, Lead Rec Type, and Opp Rec Type are not set.</description>
        <errorConditionFormula>(NOT(ISBLANK(TEXT(Lead_Record_Type__c))) || 
NOT(ISBLANK(TEXT(Opportunity_Record_Type__c))) || 
NOT(ISBLANK(TEXT(Lead_Opp_Owner_Source__c))) 
) &amp;&amp; Party_Tickets_Only__c = True</errorConditionFormula>
        <errorMessage>If Party Tickets Only is checked, then Lead Record Type, Opportunity Record Type, and Lead-Opp Owner Source should not be set.</errorMessage>
    </validationRules>
</CustomObject>
