        <deleteConstraint>SetNull</deleteConstraint>
        <description>All zone managers in the districts region are related to the region account.  One zone manager may be related to multiple districts but they have to come from the correct region.</description>
        <externalId>false</externalId>
        <label>Zone Manager</label>
        <lookupFilter>
