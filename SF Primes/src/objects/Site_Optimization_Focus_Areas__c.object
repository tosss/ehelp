    <validationRules>
        <fullName>CB_DAPFA_CategoryAndNameMustMatch</fullName>
        <active>true</active>
        <description>Stops user from editing the focus area name field unless it&apos;s on record creation.</description>
        <errorConditionFormula>AND(
Not(Isnew()),
NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)), 
Text( Category__c )&lt;&gt;  Name ,
Not(ischanged(Category__c))
)</errorConditionFormula>
        <errorDisplayField>Category__c</errorDisplayField>
        <errorMessage>Please change the Category field in order to update the Focus Area Name.</errorMessage>
    </validationRules>
