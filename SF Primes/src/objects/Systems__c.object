    <validationRules>
        <fullName>DMS_field_required</fullName>
        <active>true</active>
        <description>SLX Vendor Id required</description>
        <errorConditionFormula>ISNULL(SLXVendor_ID__c)</errorConditionFormula>
        <errorMessage>SLX DMS ID Field is required (and it should be unique to Vendors table)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SLX_VendorID_field_required</fullName>
        <active>true</active>
        <description>SLX Vendor Id required</description>
        <errorConditionFormula>ISNULL(SLXVendor_ID__c) || SLXVendor_ID__c = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>SLXVendor_ID__c</errorDisplayField>
        <errorMessage>SLX Vendor ID Field is required (and it should be unique to Vendors table)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>System_ID_field_required</fullName>
        <active>true</active>
        <description>When Phone or DMS is selected for System Type the System ID field should be populated.</description>
        <errorConditionFormula>AND(
OR( ISPICKVAL(Type__c, &quot;DMS&quot; ) ,
ISPICKVAL(Type__c, &quot;Phone&quot; ) ),
 LEN( SLXSystem_ID__c  ) =0  
)</errorConditionFormula>
        <errorDisplayField>SLXSystem_ID__c</errorDisplayField>
        <errorMessage>SLX System ID must be populated and unique for SalesLogix for DMS and Phone type systems.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_field_required</fullName>
        <active>true</active>
        <description>Type is required</description>
        <errorConditionFormula>ISPICKVAL(Type__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>The Type field is required</errorMessage>
    </validationRules>
