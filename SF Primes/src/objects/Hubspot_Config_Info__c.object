    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>DS_Mkt_CampaignEmailNameEventStatus_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISBLANK(Hubspot_Campaign_Name__c), 
OR(NOT(ISBLANK(TEXT(Hubspot_Campaign_Event_Status__c))),NOT(ISBLANK(Hubspot_Campaign_Email_Name__c))))</errorConditionFormula>
        <errorDisplayField>Hubspot_Campaign_Email_Name__c</errorDisplayField>
        <errorMessage>You can not have Campaign Email Name or Event Status for null Campaign Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Mkt_CampaignEmailNameEventStatus_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Hubspot_Campaign_Name__c)), 
OR(ISBLANK(TEXT(Hubspot_Campaign_Event_Status__c)),
ISBLANK(Hubspot_Campaign_Email_Name__c))
)</errorConditionFormula>
        <errorDisplayField>Hubspot_Campaign_Email_Name__c</errorDisplayField>
        <errorMessage>Hubspot Campaign Email Name and Event Status are Required for Hubspot Campaign</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Mkt_ContactList_Campaign_Not_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Hubspot_Contact_List_Name__c),ISBLANK(Hubspot_Campaign_Name__c))</errorConditionFormula>
        <errorDisplayField>Hubspot_Contact_List_Name__c</errorDisplayField>
        <errorMessage>You need to populate Hubspot Contact List Name or Hubspot Campaign Name for a Hubspot Record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_Mkt_ContactList_Campaign_Verification</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Hubspot_Contact_List_Name__c)),NOT(ISBLANK(Hubspot_Campaign_Name__c)))</errorConditionFormula>
        <errorDisplayField>Hubspot_Contact_List_Name__c</errorDisplayField>
        <errorMessage>You can only have Hubspot Contact List Name or Hubspot Campaign Name filled in for a Hubspot Record</errorMessage>
    </validationRules>
