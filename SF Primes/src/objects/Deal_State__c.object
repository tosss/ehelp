    <validationRules>
        <fullName>DS_BDC_Check_Opportunity_Change</fullName>
        <active>true</active>
        <description>Created for not allowing  user to change opportunity on deal state once selected</description>
        <errorConditionFormula>AND(ISCHANGED(Opportunity__c), NOT(ISBLANK(Opportunity__c)))</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>You can not change the Opportunity once selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_BDC_ContractNeededByDate</fullName>
        <active>true</active>
        <description>Created to verify Contract Needed By Date greater then or equal to current date</description>
        <errorConditionFormula>AND(OR(ISNEW() &amp;&amp; Contract_Needed_By_Date__c &lt; Today(),ISCHANGED(Contract_Needed_By_Date__c) &amp;&amp; Contract_Needed_By_Date__c &lt; Today()),NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)))</errorConditionFormula>
        <errorDisplayField>Contract_Needed_By_Date__c</errorDisplayField>
        <errorMessage>Contract Needed By Date should be greater then or equal to today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_BDC_Rush_Reason_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Rush__c = TRUE, 
ISBLANK(TEXT(Rush_Reasons__c)) 
)</errorConditionFormula>
        <errorDisplayField>Rush_Reasons__c</errorDisplayField>
        <errorMessage>Please Provide a Reason for Rush</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_BDC_Rush_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Rush__c = FALSE, 
NOT(ISBLANK(TEXT(Rush_Reasons__c))) 
)</errorConditionFormula>
        <errorDisplayField>Rush__c</errorDisplayField>
        <errorMessage>Please select Rush as Rush Reason is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DS_ExceptionContractPush_Validation</fullName>
        <active>true</active>
        <description>Checks the Exception Contract# Push must be filled when updating the Contract number once the SCS Approved and CA Approved date fields are already  filled.</description>
        <errorConditionFormula>AND(ISCHANGED(Contract_Number_s__c), NOT(ISNULL(SCS_Approved_Date__c)),  NOT(ISNULL(CA_Approved_Date__c)) ,ISBLANK(Exception_Contract_Push__c ))</errorConditionFormula>
        <errorDisplayField>Exception_Contract_Push__c</errorDisplayField>
        <errorMessage>Please enter a value in Exception Contract # Push</errorMessage>
    </validationRules>
