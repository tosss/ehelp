    <webLinks>
        <fullName>Create_Deal</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Deal</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var __FinalizedDate = &quot;{!Deal_State__c.Finalized_Deal__c}&quot;;
var __OwnerFullName = &quot;{!Deal_State__c.OwnerFullName}&quot;;
var __AccountRegion = &quot;{!Deal_State__c.Account_Region__c}&quot;;

if (__FinalizedDate != &quot;&quot;)
{
    alert(&quot;A new Deal cannot be created if the Deal State is already Finalized.&quot;);
}
else if (__OwnerFullName == &quot;SCS Deal State&quot;)
{
    alert(&quot;Please set the Owner before creating a new Deal. Owner cannot be &apos;SCS Deal State&apos;.&quot;);
}
else if (__AccountRegion == &quot;Division&quot;)
{
    alert(&quot;A new Deal cannot be created for Account Region Division.&quot;);
}
else
{
    window.open(&apos;http://{!$Setup.CustomSettings__c.SQTServer__c}home.aspx?OppID={!Deal_State__c.OpportunityId__c}&amp;UID={!$User.Id}&apos;, &apos;sqtnew&apos;, &apos;top=150,left=250,height=400,width=400,status=yes,toolbar=no,location=no,menubar=no,scrollbars=yes,resizable=yes&apos;);
}</url>
    </webLinks>
