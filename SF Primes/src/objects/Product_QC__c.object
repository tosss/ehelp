    <validationRules>
        <fullName>CB_SLS_Other_Vendor_Cannot_Be_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(Begins($User.Username, $Setup.GlobalSettings__c.Admin_Integration_Username__c)
)
&amp;&amp;
ISPICKVAL( Current_Website_Vendor__c , &quot;Other&quot;)
&amp;&amp;
ISBLANK( Other_Vendor__c )</errorConditionFormula>
        <errorDisplayField>Other_Vendor__c</errorDisplayField>
        <errorMessage>If &quot;Other&quot; is selected in the Current Website Vendor field then fill in what their current website vendor is.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_SLS_Product_Field_is_Website_Current</fullName>
        <active>true</active>
        <description>This Validation Rule makes the Current Website Vendor Field required if Product.Website is selected</description>
        <errorConditionFormula>(Product__r.Name = &apos;Website&apos;) &amp;&amp;
ISBLANK(TEXT(Current_Website_Vendor__c))</errorConditionFormula>
        <errorDisplayField>Current_Website_Vendor__c</errorDisplayField>
        <errorMessage>If Product is Website then fill in the Current Website Vendor field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_Sls_Product_QC_Level_Other</fullName>
        <active>true</active>
        <description>If Product QC Level - Make is set to Other please fill in the Other field.</description>
        <errorConditionFormula>ISPICKVAL(Product_QC_Level_Make__c, &apos;Other&apos;) &amp;&amp;
 Other__c = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>Product_QC_Level_Make__c</errorDisplayField>
        <errorMessage>If Product QC Level - Make is set to Other please fill in the Other field.</errorMessage>
