        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Expected_Upload_Date__c</fullName>
        <externalId>false</externalId>
        <label>Upload Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>MatchRate__c</fullName>
        <externalId>false</externalId>
        <formula>if(OR( 
Emails_Uploaded__c=null,Emails_Uploaded__c=0) 
,null, 
(Emails_Uploaded__c / Records_Sent__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Match Rate %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Note__c</fullName>
        <externalId>false</externalId>
        <label>Note</label>
        <length>32000</length>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Records_Sent__c</fullName>
        <externalId>false</externalId>
        <label># Records Sent</label>
        <precision>10</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Sent_to_eAppend__c</fullName>
        <externalId>false</externalId>
        <label>Sent to eAppend</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Tier__c</fullName>
        <externalId>false</externalId>
        <formula>if( OR(ISPICKVAL(eAppend_Vendor__c, &quot;eAppend1&quot;),(ISPICKVAL(eAppend_Vendor__c, &quot;eAppend4&quot;))),

if( Emails_Uploaded__c &lt;501,&quot;Tier 1&quot;,if(AND(Emails_Uploaded__c&gt;500,Emails_Uploaded__c&lt;1001),&quot;Tier 2&quot;,if(AND(Emails_Uploaded__c&gt;1000,Emails_Uploaded__c&lt;2001),&quot;Tier 3&quot;,&quot;Over 2000, Need Approval&quot;))),null)</formula>
        <label>Tier</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
