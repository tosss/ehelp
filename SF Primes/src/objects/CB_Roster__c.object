    <validationRules>
        <fullName>CB_AttritionReason_Required</fullName>
        <active>true</active>
        <description>If the Status/Role is &quot;Possible Attrition&quot;, the Attrition Reason field must be filled out.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
ISPICKVAL( Status_Role__c, &quot;Possible Attrition&quot;)&amp;&amp;
ISBLANK( TEXT(Attrition_Reason__c ))</errorConditionFormula>
        <errorDisplayField>Attrition_Reason__c</errorDisplayField>
        <errorMessage>Please provide the Attrition Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_DeskNo_Location_Requiredw_StartzTime</fullName>
        <active>true</active>
        <description>If the Start Time is filled out then the user has to fill out the Desk # or Location.</description>
        <errorConditionFormula>AND(NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)),
$Profile.Name &lt;&gt;&quot;Cobalt Cast Iron Integration&quot;,
$Profile.Name &lt;&gt; &quot;Cobalt Service Data Exporting&quot;,
$Profile.Name &lt;&gt; &quot;Cobalt Service Ops Admin&quot;,
NOT(ISBLANK(Shift_Time__c)), 
ISBLANK( Desk_or_Location__c ) 
)</errorConditionFormula>
        <errorDisplayField>Desk_or_Location__c</errorDisplayField>
        <errorMessage>Please provide Desk # or Location when the Start Time is filled out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_Explain_Required</fullName>
        <active>true</active>
        <description>If the Capacity Type is not Normal or the Capacity Override is not blank, there must be a reason given for why the value(s) was provided.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
NOT(ISBLANK(TEXT(Capacity_Type__c)))&amp;&amp;
( 
NOT(ISPICKVAL( Capacity_Type__c,&quot;Normal&quot;))||
NOT(ISBLANK( Capacity_Override__c ))
)&amp;&amp;
ISBLANK(Explain__c)</errorConditionFormula>
        <errorDisplayField>Explain__c</errorDisplayField>
        <errorMessage>Please Explain why there is a Capacity Type outside of Normal or the Capacity Override was used.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_LeaveDateRequired</fullName>
        <active>true</active>
        <description>If the Status/Role is Attrition, the Leave Date must be provided.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
ISPICKVAL(Status_Role__c,&quot;Attrition&quot;) &amp;&amp; 
ISBLANK( Leave_Date__c )</errorConditionFormula>
        <errorDisplayField>Leave_Date__c</errorDisplayField>
        <errorMessage>If the Status/Role is Attrition, you must provide the Leave Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_LeaveReasonRequired</fullName>
        <active>true</active>
        <description>If there is a Leave Date and the Leave Reason is not FMLA-out, the reason the person left must be filled out.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
ISPICKVAL(Status_Role__c,&quot;Attrition&quot;)&amp;&amp; 
NOT(ISBLANK( Leave_Date__c ))&amp;&amp;
ISBLANK(TEXT(Leave_Reason__c))</errorConditionFormula>
        <errorDisplayField>Leave_Reason__c</errorDisplayField>
        <errorMessage>Please provide the reason why the employee left.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_RampStartDate_RequiredWhenFMLA</fullName>
        <active>true</active>
        <description>If the Status/Role is FMLA-In, please provide the Ramp Start Date.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
ISPICKVAL( Status_Role__c ,&quot;FMLA-In&quot;)&amp;&amp;
ISBLANK( Ramp_Start_Date__c )</errorConditionFormula>
        <errorDisplayField>Ramp_Start_Date__c</errorDisplayField>
        <errorMessage>If the Status/Role is FMLA-In, a Ramp Start Date must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_RampStartDate_Requiredw_StartDate</fullName>
        <active>true</active>
        <description>If the Start Date is filled out, the user has to fill out the Ramp Date.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
NOT(ISBLANK( Start_Date__c ))&amp;&amp;
ISBLANK( Ramp_Start_Date__c )</errorConditionFormula>
        <errorDisplayField>Ramp_Start_Date__c</errorDisplayField>
        <errorMessage>Please provide the Ramp Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_RequesitionStatus_Open_MustHaveReason</fullName>
        <active>true</active>
        <description>When a Requisition Status of Pending Approval is selected, the user must provide more information in the Requisition Status Reason field.</description>
        <errorConditionFormula>AND(NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)), 
$Profile.Name &lt;&gt;&quot;Cobalt Service Data Exporting&quot;, 
$Profile.Name &lt;&gt;&quot;Cobalt Cast Iron Integration&quot;, 
$Profile.Name &lt;&gt; &quot;Cobalt Service Ops Admin&quot;, 
ISPICKVAL( Requisition_Status__c , &quot;Pending Approval&quot;), 
ISBLANK( Requisition_Status_Reason__c ) 
)</errorConditionFormula>
        <errorDisplayField>Requisition_Status_Reason__c</errorDisplayField>
        <errorMessage>If you choose a Requisition Status of &quot;Pending Approval,&quot; you must provide why in the Requisition Status Reason field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_StatusRole_NewHire_DatesNeeded</fullName>
        <active>true</active>
        <description>If the Status/Role field is New Hire, the Start Date and Ramp Start Date fields must be populated.</description>
        <errorConditionFormula>NOT($Profile.Name = &quot;Cobalt Service Ops Admin&quot;)&amp;&amp; 
NOT($Profile.Name = &quot;Cobalt Service Data Exporting&quot;)&amp;&amp; 
ISPICKVAL( Status_Role__c ,&quot;New Hire&quot;)&amp;&amp; 
(
ISBLANK(Start_Date__c)||
ISBLANK(Ramp_Start_Date__c)
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>If the Status/Role is New Hire, provide the Start Date and Ramp Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CB_WorkLocation_MustHaveDaysinOffice</fullName>
        <active>true</active>
        <description>When the Work Location is &quot;Desk Share&quot; please fill in the Days in Office.</description>
        <errorConditionFormula>AND(NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)), 
$Profile.Name &lt;&gt;&quot;Cobalt Service Data Exporting&quot;, 
$Profile.Name &lt;&gt;&quot;Cobalt Cast Iron Integration&quot;, 
$Profile.Name &lt;&gt; &quot;Cobalt Service Ops Admin&quot;, 
ISPICKVAL(Work_Location__c,&quot;Desk Share&quot;), 
ISBLANK( Days_in_Office__c ) 
)</errorConditionFormula>
        <errorDisplayField>Days_in_Office__c</errorDisplayField>
        <errorMessage>Please fill in the Days in Office when the Work Location is &quot;Desk Share&quot;.</errorMessage>
    </validationRules>
</CustomObject>
