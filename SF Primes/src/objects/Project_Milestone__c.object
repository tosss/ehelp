        <fullName>Cannot_Edit_Referenced_Definition</fullName>
        <active>true</active>
        <description>An assigned definition cannot be modified.</description>
        <errorConditionFormula>PRIORVALUE(Milestone_Definition__c) != null &amp;&amp;  ISCHANGED(Milestone_Definition__c)</errorConditionFormula>
        <errorMessage>You cannot modify an existing record&apos;s referenced definition.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Milestone_RMSOwned_Null_SubProjectSite</fullName>
        <active>true</active>
        <description>Prevent Milestone creation if RMS Owned is true and it has no SubProjectSite</description>
        <errorConditionFormula>RMS_Owned_Flag__c = true &amp;&amp; ISBLANK(Site__c) &amp;&amp;  ISNEW()</errorConditionFormula>
        <errorMessage>Milestone cannot be created if it is RMS Owned and Site is blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Edit_RMSRecord_Milestone</fullName>
        <active>true</active>
        <description>Prevent specified fields for RMS-Owned Records (i.e. where RMS_Owned_Flag__c = true) from being updated unless updated by Integration User or a System Administrator</description>
        <errorConditionFormula>AND
(
RMS_Owned_Flag__c == true , OR(ISCHANGED( SubProject__c ) , ISCHANGED( Description__c )),
NOT($Profile.Name == &apos;ADPDS - Admin - System Administrator&apos;),
NOT($Profile.Name == &apos;ADPDS - Admin - Integration User&apos;),
NOT($Profile.Name == &apos;Cobalt Cast Iron Integration&apos;)

)</errorConditionFormula>
        <errorMessage>This record is owned by RMS. Contact an RMS scheduler to change the CDK Assignee.  Other updates should be performed by the owner of the task via the My RMS Assignments tab in CDK Expressway.</errorMessage>
    </validationRules>
