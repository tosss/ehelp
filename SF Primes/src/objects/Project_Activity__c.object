    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Actual_End_Date_Before_Start</fullName>
        <active>true</active>
        <description>Actual End Date cannot occur before Actual Start Date.</description>
        <errorConditionFormula>IF(( NOT(ISBLANK(Actual_End_Date__c)) &amp;&amp; NOT(ISBLANK(Actual_Start_Date__c)) ), ( Actual_End_Date__c &lt; Actual_Start_Date__c ), FALSE)</errorConditionFormula>
        <errorMessage>Actual End Date cannot occur before Actual Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assign_Associate_before_Actual_Dates</fullName>
        <active>true</active>
        <description>An associate (CDK Assignee ) needs to be entered prior to inputting Actual Dates.</description>
        <errorConditionFormula>AND(NOT(CONTAINS($Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c, $User.FirstName + &apos; &apos; + $User.LastName)),ISBLANK(CDK_Assignee__c) &amp;&amp; NOT(ISBLANK(Actual_Start_Date__c)), 
$Profile.Name != &apos;ADPDS - Admin - Integration User&apos; || $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;)</errorConditionFormula>
        <errorMessage>A CDK Assignee needs to be filled-in prior to entering Actual Start or End Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Create_Activity_if_Milestone_Comp</fullName>
        <active>true</active>
        <description>Cannot Create Project Activity Record if Project Milestone is Complete</description>
        <errorConditionFormula>AND (
        NOT( CONTAINS($Setup.CustomSettings__c.Users_Excluded_from_Business_Rules__c, $User.FirstName + &apos; &apos; + $User.LastName) ),
        $Profile.Name != &apos;ADPDS - Admin - Integration User&apos; ||   $Profile.Name != &apos;Cobalt Cast Iron Integration&apos;, 
        Milestone__r.Status__c = &apos;Complete&apos;, ISNEW()
    )</errorConditionFormula>
        <errorMessage>You cannot create a Project Activity record if its parent Project Milestone is already Complete.  Please remove the record that you’re trying to create.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Referenced_Definition</fullName>
        <active>true</active>
        <description>An assigned definition cannot be modified.</description>
        <errorConditionFormula>PRIORVALUE(Activity_Definition__c ) != null &amp;&amp; ISCHANGED(Activity_Definition__c )</errorConditionFormula>
        <errorMessage>You cannot modify an existing record&apos;s referenced definition.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_before_Start_Date</fullName>
        <active>true</active>
        <description>Cannot save a record with Planned/Actual End Date before setting Planned/Actual Start Date.</description>
        <errorConditionFormula>( ISBLANK(Actual_Start_Date__c) &amp;&amp; NOT( ISBLANK(Actual_End_Date__c)) ) || ( ISBLANK(Planned_Start_Date__c) &amp;&amp; NOT( ISBLANK(Planned_End_Date__c)) )</errorConditionFormula>
        <errorMessage>Planned / Actual Start Date needs to be specified before specifying a Planned / Actual End Date for the Activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Future_Actual_Start_End_Dates</fullName>
        <active>true</active>
        <description>Does not allow Actual Start Date or Actual End Date to be in the future.</description>
        <errorConditionFormula>( Actual_Start_Date__c &gt; TODAY() ) || ( Actual_End_Date__c &gt; TODAY() )</errorConditionFormula>
        <errorMessage>Actual Start and End Dates cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planned_End_Date_Before_Start</fullName>
        <active>true</active>
        <description>Planned End Date cannot occur before Planned Start Date.</description>
        <errorConditionFormula>IF( ( NOT(ISBLANK(Planned_End_Date__c)) &amp;&amp; NOT(ISBLANK(Planned_Start_Date__c)) ),( Planned_End_Date__c &lt; Planned_Start_Date__c ), FALSE )</errorConditionFormula>
        <errorMessage>Planned End Date cannot occur before Planned Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Edit_RMSRecord_Activity</fullName>
        <active>true</active>
        <description>Prevent specified fields for RMS-Owned Records (i.e. where RMS_Owned_Flag__c = true) from being updated unless updated by Integration User or a System Administrator</description>
        <errorConditionFormula>AND
(
	RMS_Owned_Flag__c == true ,  
	OR(ISCHANGED(  Description__c ), ISCHANGED(  CDK_Assignee__c ), ISCHANGED(  Planned_Start_Date__c ), ISCHANGED(  Planned_End_Date__c ) , 
        ISCHANGED( Milestone__c ) ), 	
	NOT($Profile.Name == &apos;ADPDS - Admin - System Administrator&apos;),
	NOT($Profile.Name == &apos;ADPDS - Admin - Integration User&apos;),
        NOT($Profile.Name == &apos;Cobalt Cast Iron Integration&apos;)
	 
)</errorConditionFormula>
        <errorMessage>This record is owned by RMS. Contact an RMS scheduler to change the CDK Assignee.  Other updates should be performed by the owner of the task via the My RMS Assignments tab in CDK Expressway.</errorMessage>
    </validationRules>
</CustomObject>
