    <fields>
        <fullName>Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Last Activity records are related to either an Account, Lead, or Opportunity. If an Activity (Event or Task) occurred on an Account or Campaign, a Last Activity record is created with this Account relationship set to the Account. The &quot;Related To&quot; field is also set (for reporting) to the Account for all Last Activity records.</description>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Last Activities</relationshipLabel>
        <relationshipName>Last_Activities</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
