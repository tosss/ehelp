    <rules>
        <fullName>DS_Lsr_Out_Laser_Artwork_Status_Change</fullName>
        <actions>
            <name>LAO_Status_Chg_Workflow</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <description>Laser Artwork Status Change - fire web service call</description>
        <formula>ISNEW() || (ISCHANGED( Order_Status__c ) &amp;&amp; (  CASE( Order_Status__c , &quot;Approved&quot;, &quot;Approved&quot;, &quot; &quot;) = &quot;Approved&quot; || CASE( Order_Status__c , &quot;Approved-FA&quot;, &quot;Approved-FA&quot;, &quot; &quot;) = &quot;Approved-FA&quot; || CASE( Order_Status__c , &quot;Artwork to LFD Team&quot;, &quot;Artwork to LFD Team&quot;, &quot; &quot;) = &quot;Artwork to LFD Team&quot; || CASE( Order_Status__c , &quot;At LIS Team&quot;, &quot;At LIS Team&quot;, &quot; &quot;) = &quot;At LIS Team&quot; || CASE( Order_Status__c , &quot;Awaiting Approval&quot;, &quot;Awaiting Approval&quot;, &quot; &quot;) = &quot;Awaiting Approval&quot; || CASE( Order_Status__c , &quot;Awaiting Approval-ELF Data&quot;, &quot;Awaiting Approval-ELF Data&quot;, &quot; &quot;) = &quot;Awaiting Approval-ELF Data&quot; || CASE( Order_Status__c , &quot;Awaiting Approval-FA&quot;, &quot;Awaiting Approval-FA&quot;, &quot; &quot;) = &quot;Awaiting Approval-FA&quot; || CASE( Order_Status__c , &quot;AWIP&quot;, &quot;AWIP&quot;, &quot; &quot;) = &quot;AWIP&quot; || CASE( Order_Status__c , &quot;Binder Sent&quot;, &quot;Binder Sent&quot;, &quot; &quot;) = &quot;Binder Sent&quot; || CASE( Order_Status__c , &quot;Burned&quot;, &quot;Burned&quot;, &quot; &quot;) = &quot;Burned&quot; || CASE( Order_Status__c , &quot;Forms Analysis&quot;, &quot;Forms Analysis&quot;, &quot; &quot;) = &quot;Forms Analysis&quot; || CASE( Order_Status__c , &quot;Forms to LIS Team&quot;, &quot;Forms to LIS Team&quot;, &quot; &quot;) = &quot;Forms to LIS Team&quot; || CASE( Order_Status__c , &quot;Installed&quot;, &quot;Installed&quot;, &quot; &quot;) = &quot;Installed&quot; || CASE( Order_Status__c , &quot;Proofs to LAS&quot;, &quot;Proofs to LAS&quot;, &quot; &quot;) = &quot;Proofs to LAS&quot; || CASE( Order_Status__c , &quot;Q/S Approved&quot;, &quot;Q/S Approved&quot;, &quot; &quot;) = &quot;Q/S Approved&quot; || CASE( Order_Status__c , &quot;Q/S Submitted&quot;, &quot;Q/S Submitted&quot;, &quot; &quot;) = &quot;Q/S Submitted&quot; || CASE( Order_Status__c , &quot;Sent to Data Mapping/Other&quot;, &quot;Sent to Data Mapping/Other&quot;, &quot; &quot;) = &quot;Sent to Data Mapping/Other&quot; || CASE( Order_Status__c , &quot;Sold&quot;, &quot;Sold&quot;, &quot; &quot;) = &quot;Sold&quot; || CASE( Order_Status__c , &quot;Sold-PreFA&quot;, &quot;Sold-PreFA&quot;, &quot; &quot;) = &quot;Sold-PreFA&quot;) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Email - LAO - Owner Change Notification</fullName>
        <actions>
            <name>Alert_to_LAO_Owner_Change_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>LAO Owner Change Notification</description>
        <formula>And ( ISCHANGED( OwnerId ), NOT(BEGINS($User.Username,$Setup.GlobalSettings__c.Admin_Integration_Username__c)),   NOT(OR(ISPICKVAL(Order_Type__c,&quot;Add/Change - Elite&quot;),ISPICKVAL(Order_Type__c,&quot;Buy/Sell - Elite&quot;),ISPICKVAL(Order_Type__c,&quot;New Install - Elite&quot;), ISPICKVAL(Order_Type__c,&quot;Add/Change - ELF&quot;),  ISPICKVAL(Order_Type__c,&quot;Buy/Sell - ELF&quot;), ISPICKVAL(Order_Type__c,&quot;New Install - ELF&quot;),ISPICKVAL(Order_Type__c,&quot;Add/Change - DASH&quot;),  ISPICKVAL(Order_Type__c,&quot;Buy/Sell - DASH&quot;), ISPICKVAL(Order_Type__c,&quot;New Install - DASH&quot;))), NOT(ISPICKVAL(Status__c,&quot;Sent to FB and/or Complete&quot;)),OwnerId &lt;&gt; &apos;005400000013SSW&apos;,OwnerId&lt;&gt;&apos;005400000013SSK&apos;,OwnerId&lt;&gt;&apos;00540000000zuMN&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
