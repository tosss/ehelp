        </actions>
        <active>true</active>
        <description>Created to sent email to LIS Static email and update owner to Lucy Colella for Order Status Artwork To LFD Team and Elite, ELF and DASH order types</description>
        <formula>AND(RecordType.Name=&quot;Laser Imaging&quot;,OR(ISCHANGED(Order_Type__c), ISCHANGED(Order_Status__c),ISNEW()), OR(ISPICKVAL(Order_Type__c,&quot;Buy/Sell - Elite&quot;), ISPICKVAL(Order_Type__c,&quot;New Install - Elite&quot;), ISPICKVAL(Order_Type__c,&quot;Buy/Sell - ELF&quot;), ISPICKVAL(Order_Type__c,&quot;New Install - ELF&quot;),ISPICKVAL(Order_Type__c,&quot;New Install - DASH&quot;), ISPICKVAL(Order_Type__c,&quot;Buy/Sell - DASH&quot;)), ISPICKVAL(Order_Status__c,&quot;Artwork to LFD Team&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
