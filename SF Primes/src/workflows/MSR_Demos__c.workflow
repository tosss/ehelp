    <rules>
        <fullName>CB_SLS_Demo_Performed_Update_Status</fullName>
        <actions>
            <name>CB_SLS_Demo_Performed_Update_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Updates the Status to Complete when the Date Demo is Completed is filled in.</description>
        <formula>NOT(Begins($User.Username, $Setup.GlobalSettings__c.Admin_Integration_Username__c) ) &amp;&amp;   NOT(isblank(Date__c))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CB_SLS_SC_Assigned_Notification</fullName>
        <actions>
            <name>CB_SLS_SC_Assigned_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>MSR_Demos__c.MSR__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>MSR_Demos__c.Type_of_Demo__c</field>
            <operation>equals</operation>
            <value>Remote</value>
        </criteriaItems>
        <criteriaItems>
            <field>MSR_Demos__c.Status__c</field>
            <operation>notEqual</operation>
            <value>Completed</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CB_SLS_Set_SC_Assigned_Date</fullName>
        <actions>
            <name>CB_SLS_SC_Assigned_Set</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CB_SLS_Status_Set</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>NOT(Begins($User.Username, $Setup.GlobalSettings__c.Admin_Integration_Username__c))&amp;&amp; NOT(ISBLANK(MSR__c)) &amp;&amp; CB_SLS_Created_frm_Chatter_Action_Hidden__c = TRUE</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CB_SLS_Set_SC_Assigned_Date is Cleared</fullName>
        <actions>
            <name>CB_SLS_Set_SC_Assigned_Date_is_Cleared_Email_Alert</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>CB_SLS_Set_SC_Assigned_Date_is_Cleared</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CB_SLS_Status_Set_to_Pending</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>NOT(Begins($User.Username, $Setup.GlobalSettings__c.Admin_Integration_Username__c))&amp;&amp; (ISBLANK(MSR__c)) &amp;&amp; IsPickVal(Status__c, &quot;Assigned Solution Consultant&quot;) &amp;&amp; IsPickVal( Type_of_Demo__c,&quot;Remote&quot;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CB_SLS_Type_of_Demo_Remote_Demo</fullName>
        <actions>
            <name>CB_SLS_SCP_Alias_SCP_Record_Created_by_Remote_Demo_Action</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>CB_SLS_Type_of_Demo_Remote_Demo</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>MSR_Demos__c.CB_SLS_Created_frm_Chatter_Action_Hidden__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Sets the Type of Demo field to be &quot;Remote&quot; when created via the Remote Demo Chatter action on the Opportunity Object</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
