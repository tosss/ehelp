    <rules>
        <fullName>DS_Solution_Consultant_Survey_Not Received</fullName>
        <active>true</active>
        <criteriaItems>
            <field>MSR_Demos__c.Survey_Sent_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>MSR_Demos__c.Survey_Completed_Date_and_Time__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>MSR_Demos__c.Type_of_Demo__c</field>
            <operation>equals</operation>
            <value>Remote</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>DS_Solution_Consultant_Presentation_Email_Alert_Reminder</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>MSR_Demos__c.Survey_Sent_Date__c</offsetFromField>
            <timeLength>3</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
