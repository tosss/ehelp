                            <!--<ui:inputDate label="" class="dateStyle" value="{!v.recWrapper.newTask.ActivityDate}" displayDatePicker="true"/>-->
                            <c:DateTimePicker label="" value="{!v.recWrapper.newTask.ActivityDate}" datetime="false"/>
                        </div>
                    </div>
                    
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Email</div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <span class="slds-form-element__static">
                                <ui:outputEmail value="{!v.recWrapper.contactRecord.Email}"/>
                            </span>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap slds-grid--pull-padded slds-col--bump-right slds-col--bump-left customMatchGridFields">
