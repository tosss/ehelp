                            <div class="slds-truncate">
                                <span class="slds-form-element__static">
                                    <b><ui:outputText value="Completed Activity Information"/></b>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap slds-grid--pull-padded slds-col--bump-right slds-col--bump-left customMatchGridFields">
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Assigned To<abbr class="slds-required" title="required">*</abbr></div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <c:ALL_LookUp objectAPIName="User" aura:id="taskAssignedTo" selectedId="{!v.recWrapper.newTask.OwnerId}" label=""/>
                        </div>
                    </div>
                    
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Task Record Type</div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
