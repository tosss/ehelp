<!--2016-04-01- anirudh.singh@cdk.com- Created this lightning component for Request RQ-06734 Convert Activity_Opportunity Page to Lightning.-->
<aura:component implements="force:appHostable" controller="SLS_ActivityOpportunity_Controller">
    <ltng:require styles="/resource/slds100/assets/styles/salesforce-lightning-design-system.css"/>
    <ltng:require styles="/resource/FontAwesome/4.3.0/css/font-awesome.min.css"/>
    <ltng:require scripts="/resource/jQuery/js/jquery-1.10.2.min.js"/>
    
    <aura:attribute name="leadId" type="Id" default="00Q4000001420NVEAY"/>
    <aura:attribute name="accountId" type="Id" default="0014000000Jw8wl"/>
    <aura:attribute name="contactId" type="Id" default="0031b000004Syqt"/>
    <aura:attribute name="recWrapper" type="SLS_ActivityOpportunity_Controller.RecordsWrapper"/>
    <aura:attribute name="hardware" type="String" default="{!$Browser.formFactor}"/>
    <aura:attribute name="errorMessageValue" type="String"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.initialize}"/>
    
    <!--Spinner-->
    <div>
        <div aura:id="spinnerActivityOpportunity" class="slds-spinner_container slds-hide">
            <div class="slds-spinner--brand slds-spinner slds-spinner--large">
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </div>
    
    <!--Header-->
    <div>
        <div class="slds-page-header" role="banner">
            <div class="slds-media">
                <div class="slds-media__figure">
                    <img class="logoStyle" src="/resource/slds100/assets/icons/standard/call_60.png"/>
                </div>
                <div class="slds-media__body">
                    <p class="slds-page-header__title slds-align-middle" title="Log a Call"><b>Log a Call</b></p>
                    <p class="slds-text-body--small slds-page-header__info" title="Task, Follow-up and Campaign Member Status">Task, Follow-up and Campaign Member Status</p>
                </div>
            </div>
        </div>
    </div>
    
    <ui:message aura:id="errorMessage" title="Error" severity="error" class="slds-hide">
    </ui:message>
    
    <div>
        <div class="pageButtonStyle">
            <center>
                <button class="slds-button slds-button--brand slds-not-selected" type="button" onclick="{!c.saveChanges}">Save</button>
                <button class="slds-button slds-button--brand slds-not-selected" type="button" onclick="{!c.cancelChanges}">Cancel</button>
            </center>
        </div>
    </div>
    
    <!--Completed Activity Information-->
    <div>
        <div class="{!v.recWrapper.showCompletedActivitySection==true? '':'slds-hide'}">
