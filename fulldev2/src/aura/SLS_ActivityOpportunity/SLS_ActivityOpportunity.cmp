                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Type</div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <div class="{!v.recWrapper.renderForCobalt == true ? '' : 'slds-hide'}">
                                <div class="slds-select_container">
                                    <!--<ui:inputSelect class="dynamic slds-select" aura:id="taskTypeMap" value="{!v.recWrapper.newTask.Type}"/>-->
                                    <aura:if isTrue="{!v.hardware=='DESKTOP'}">
                                        <ui:inputSelect class="dynamic slds-select" aura:id="taskTypeMap" value="{!v.recWrapper.newTask.Type}"/>
                                        <aura:set attribute="else">
                                            <!--<force:inputField class="" value="{!v.recWrapper.newTask.Type}"/>-->
                                            test type
                                        </aura:set>
                                    </aura:if>
                                </div>
                            </div>
                            <div class="{!v.recWrapper.renderForDS == true ? '' : 'slds-hide'}">
                                <ui:inputText class="slds-input" value="{!v.recWrapper.newTask.Activity_Type__c}" placeholder="Please enter a Task Type..."/>
                            </div>
                        </div>
                    </div>
                    
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Contact Phone</div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <span class="slds-form-element__static">
                                <ui:outputPhone value="{!v.recWrapper.contactRecord.Phone}"/>
                            </span>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap slds-grid--pull-padded slds-col--bump-right slds-col--bump-left customMatchGridFields">
