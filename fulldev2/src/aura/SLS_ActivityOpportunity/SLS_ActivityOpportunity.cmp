                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Subject<abbr class="slds-required" title="required">*</abbr></div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <div class="slds-select_container" aura:id="taskSubjectOptionsMapDiv">
                                <!--<ui:inputSelect class="{!'dynamic slds-select '+(v.hardware != 'DESKTOP' ? 'slds-hide' : '')}" aura:id="taskSubjectOptionsMap" value="{!v.recWrapper.newTask.Subject}" required="true"/>-->
                                <aura:if isTrue="{!v.hardware=='DESKTOP'}">
                                    <ui:inputSelect class="dynamic slds-select" aura:id="taskSubjectOptionsMap" value="{!v.recWrapper.newTask.Subject}" required="true"/>
                                    <aura:set attribute="else">
                                        <!--<force:inputField class="" value="{!v.recWrapper.newTask.Subject}"/>-->
                                        test subject
                                    </aura:set>
                                </aura:if>
                            </div>
                        </div>
                    </div>
                    
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Name<abbr class="slds-required" title="required">*</abbr></div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <c:ALL_LookUp objectAPIName="Lead" aura:id="taskWhoId" selectedId="{!v.recWrapper.newTask.WhoId}" label=""/>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap slds-grid--pull-padded slds-col--bump-right slds-col--bump-left customMatchGridFields">
