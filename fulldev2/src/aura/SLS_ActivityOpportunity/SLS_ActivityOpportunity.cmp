                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <ui:inputTextArea class="slds-textarea" value="{!v.recWrapper.newTask.Description}" rows="2"/>
                        </div>
                    </div>
                    
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="slds-text-heading--label customMatchColPadding customMatchHeading">Status</div>
                    </div>
                    <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-large-size--1-of-4">
                        <div class="customMatchColPadding">
                            <span class="slds-form-element__static">
                                <ui:outputText value="{!v.recWrapper.newTask.Status}"/>
                            </span>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-grid--align-center customMatchGridButton">
                    <button class="{!'slds-button slds-button--brand slds-not-selected'+' '+(v.recWrapper.showEventSection==true? '':'slds-hide')}" type="button" onclick="{!c.showHideEventSection}">Follow-up Not Needed</button>
