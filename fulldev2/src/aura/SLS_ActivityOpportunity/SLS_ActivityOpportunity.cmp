                            <c:ALL_LookUp objectAPIName="Lead" selectedId="{!v.recWrapper.newEvent.WhoId}" label=""/>
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap slds-grid--pull-padded slds-col--bump-right slds-col--bump-left customMatchGridFields">
