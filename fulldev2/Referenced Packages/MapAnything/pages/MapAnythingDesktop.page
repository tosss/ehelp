                                            else
                                            {
                                                TempArray.push('<div action="' + action.action + '" onclick="MALayers.layerActionsClickHandler(this);">' + action.label + '</div>');
                                            }
                                            */
                                        }
                                    }); 
                                            
                                    // console.log('options:',options);
                                    // console.log('TempArray:',TempArray);
                                    
            	                    return TempArray;
