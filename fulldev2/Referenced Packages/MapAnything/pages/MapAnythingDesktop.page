            					{
            						var nextCoord = coords.splice(0, 1)[0];
            					
            						var iconSVG = '<svg width="12" height="12" xmlns="http://www.w3.org/2000/svg">' +
										'<circle opacity="0.8" fill="#006eff" cx="3" cy="3" r="6" id="svg_3" stroke="#ffffff" stroke-width="2"/>' +
